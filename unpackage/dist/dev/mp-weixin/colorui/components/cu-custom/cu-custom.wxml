<view><view class="cu-custom" style="{{'height:'+(CustomBar+'px')+';'}}"><view class="{{['cu-bar fixed',bgImage!=''?'none-bg text-white bg-img':'',bgColor]}}" style="{{(style)}}"><block wx:if="{{isBack}}"><view data-event-opts="{{[['tap',[['BackPage',['$event']]]]]}}" class="action" bindtap="__e"><text class="{{['cuIcon-'+icon]}}"></text><slot name="backText"></slot></view></block><view class="content" style="{{'top:'+(StatusBar+'px')+';'}}"><slot name="content"></slot></view><slot name="right"></slot></view></view><tui-drawer vue-id="1" mode="left" visible="{{leftDrawer}}" data-event-opts="{{[['^close',[['closeDrawer']]]]}}" bind:close="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="d-container h-100 flex flex-direction justify-center align-center"><view class="cu-avatar xl bg-red round cu-card shadow margin-bottom-xl"><image class="w-100 h-100" src="http://api.btstu.cn/sjtx/api.php"></image></view><view class="cu-list w-100 menu"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item arrow" hover-class="hover-class" data-event-opts="{{[['tap',[['handleNav',['$0'],[[['navList','',index]]]]]]]}}" bindtap="__e"><view class="content"><text class="{{['text-grey','cuIcon-'+item.icon]}}"></text><text class="text-grey">{{item.navName}}</text></view></view></block></view></view></tui-drawer></view>